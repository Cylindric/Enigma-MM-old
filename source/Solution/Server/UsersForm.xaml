    <Window x:Class="EnigmaMM.UsersForm"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
            xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
            Title="Users" Height="287" Width="442">
        <Grid>
            <DataGrid AutoGenerateColumns="False" Height="200" HorizontalAlignment="Left" Margin="44,22,0,0" Name="dataGrid1" VerticalAlignment="Top" Width="200" ItemsSource="{Binding}">
                <DataGrid.Columns>
                    <DataGridTextColumn Header="User ID" Binding="{Binding Path=User_ID}" />
                    <DataGridTextColumn Header="Username" Binding="{Binding Path=Username}" />
                    <DataGridTextColumn Header="Last Seen" Binding="{Binding Path=LastSeen}" />
                </DataGrid.Columns>
            </DataGrid>
        </Grid>
    </Window>
