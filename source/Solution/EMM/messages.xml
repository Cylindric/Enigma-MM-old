<?xml version="1.0"?>
<messages>
	<message>
  	<name>StartupComplete</name>
		<type>EndsWith</type>
		<rule>[INFO] Done! For help, type "help" or "?"</rule>
	</message>
  <message>
		<name>SaveComplete</name>
		<type>EndsWith</type>
		<rule>[INFO] CONSOLE: Save Complete</rule>
	</message>
  <message>
		<name>ErrorPortBusy</name>
		<type>EndsWith</type>
		<rule>[WARNING] **** FAILED TO BIND TO PORT!</rule>
	</message>
  <message>
		<name>SaveStarted</name>
		<type>EndsWith</type>
		<rule>[INFO] CONSOLE: Forcing save..</rule>
	</message>
  <message>
		<name>UserLoggedIn</name>
		<type>Regex</type>
		<rule>^(?&lt;timestamp&gt;.+?)\[INFO]\ (?&lt;username&gt;\w+?)\ \[(?&lt;address&gt;.+?)]\ logged\ in with entity id (?&lt;entityid&gt;.*?)$</rule>
	</message>
  <message>
		<name>UserLoggedOut</name>
		<type>Regex</type>
		<rule>^(?&lt;timestamp&gt;.+?)\[INFO]\ (?&lt;username&gt;\w+?)\ lost\ connection:\ (?&lt;reason&gt;.+?)$</rule>
	</message>
  <message>
		<name>AutoSaveEnabled</name>
		<type>EndsWith</type>
		<rule>[INFO] CONSOLE: Enabling level saving..</rule>
	</message>
  <message>
		<name>AutoSaveDisabled</name>
		<type>EndsWith</type>
		<rule>[INFO] CONSOLE: Disabling level saving..</rule>
	</message>
  <message>
		<name>UserList</name>
		<type>Regex</type>
		<rule>^(?&lt;timestamp&gt;.+?)\[INFO]\ Connected\ players:\ (?&lt;userlist&gt;.*?)$</rule>
	</message>
  <message>
		<name>UserCount</name>
		<type>Regex</type>
		<rule>^Player\ count:\ (?&lt;count&gt;\d+)$</rule>
	</message>
  <message>
		<name>MinecraftBanner</name>
		<type>Regex</type>
		<rule>^(?&lt;timestamp&gt;.+?)\[INFO]\ Starting\ minecraft\ server\ version\ (?&lt;version&gt;.*?)$</rule>
	</message>
</messages>